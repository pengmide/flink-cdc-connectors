        <executions>
          <execution>
            <id>default-test</id>
            <phase>none</phase>
          </execution>
          <execution>
            <id>integration-tests</id>
            <phase>none</phase>
          </execution>
          <execution>
            <id>end-to-end-tests</id>
            <phase>integration-test</phase>
            <goals>
              <goal>test</goal>
            </goals>
            <configuration>
              <includes>
                <include>**/*.*</include>
              </includes>
              <forkCount>1</forkCount>
              <systemPropertyVariables>
                <moduleDir>${project.basedir}</moduleDir>
              </systemPropertyVariables>
            </configuration>
          </execution>
        </executions>
      </plugin>
